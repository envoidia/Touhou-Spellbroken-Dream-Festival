function<string> FrameToHMS(int frame_)
{
	int h, m, s;
	int sec = frame_ ~/ 60;
	s = sec % 60;
	m = (sec ~/ 60) % 60;
	h = sec ~/ 60 ~/ 60;
	return StringFormat("%3d:%02d:%02d", "ddd", h, m, s);
}

function<string> FrameToMS(string frame_)
{
	int m, s;
	int frame = atoi(frame_);
	int sec = frame ~/ 60;
	s = sec % 60;
	m = (sec ~/ 60);
	return StringFormat("%02d:%02d", "dd", m, s);
}

