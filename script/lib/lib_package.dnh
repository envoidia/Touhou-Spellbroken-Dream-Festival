	string score = DigitToCommaArray(GetAreaCommonData(AREA_HISTORY, SPELL_HISCORE ~ itoa(spellID) ~ _player, 100000000i));
	
	// To view all comments for debug purposes, change (numCap > 0) to (numCap > -1)
